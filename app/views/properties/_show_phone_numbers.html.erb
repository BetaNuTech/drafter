<div class="row">
  <div class="col-md-12">
    <h2>Marketing Phone Numbers</h2>

    <% @property.phone_numbers.each do |phone| %>
      <div class="row">
        <div class="col-md-12">
          <h4 style="display: inline;"><%= phone.name %>:</h4>
          +<%= phone.prefix %> <%= number_to_phone(phone.number) %>
        </div>
      </div>
    <% end %>
  </div>
</div>
